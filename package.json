{
  "name": "piggy-wallet",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "format:check": "prettier --list-different \"**/*.{ts,tsx,json}\"",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest -u",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "json-server": "json-server --watch db.json --port 8001",
    "json-server:tunnel": "json-server --watch db.json --port 8000 --host 0.0.0.0",
    "tunnel": "expo start --tunnel",
    "start:prod": "expo start --no-dev",
    "tunnel:prod": "expo start --no-dev --tunnel",
    "prebuild": "expo prebuild",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:android": "expo prebuild --platform android"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "axios": "^1.6.2",
    "date-fns": "^3.6.0",
    "expo": "^51.0.8",
    "expo-router": "~3.5.14",
    "expo-secure-store": "~13.0.1",
    "expo-status-bar": "~1.12.1",
    "formik": "^2.4.5",
    "json-server": "^1.0.0-beta.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-animated-nav-tab-bar": "^3.1.10",
    "react-native-ionicons": "^4.6.5",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-picker-select": "^9.1.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-typography": "^1.4.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.6",
    "victory-native": "^36.8.4",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.79",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
